<Window x:Class="ISFF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ISFF"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="800" Style="{DynamicResource WindowStyle}"
        WindowStartupLocation="CenterScreen" MinWidth="800" MinHeight="400">
    <Grid>
        <StackPanel>
            <Button Style="{DynamicResource BigButtonStyle}">Продукты</Button>
            <Button Style="{DynamicResource SmallButtonStyle}">Товары</Button>
            <Button Style="{DynamicResource BigButtonStyle}">Сотрудники</Button>
            <Button Style="{DynamicResource SmallButtonStyle}">Заказы</Button>
            <TextBlock Style="{DynamicResource TextblockStyle}">FastF</TextBlock>
            <TextBox Style="{DynamicResource TextboxStyle}">Go go go</TextBox>
            <ComboBox Style="{DynamicResource ComboboxStyle}">Elae</ComboBox>
        </StackPanel>
    </Grid>
</Window>
