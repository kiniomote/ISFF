<Window x:Class="ISFF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ISFF"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="800" Style="{DynamicResource WindowStyle}">
    <Grid>
        <StackPanel>
            <Button Style="{DynamicResource ButtonStyle}">Продукты</Button>
            <Button Style="{DynamicResource ButtonStyle}">Товары</Button>
            <Button Style="{DynamicResource ButtonStyle}">Сотрудники</Button>
            <Button Style="{DynamicResource ButtonStyle}">Заказы</Button>
        </StackPanel>
    </Grid>
</Window>
